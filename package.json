{
  "name": "@bkoplin/utils",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.12.0",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @bkoplin",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/bkoplin",
  "homepage": "https://github.com/bkoplin/utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bkoplin/utils.git"
  },
  "bugs": {
    "url": "https://github.com/bkoplin/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": true,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.4",
    "@antfu/ni": "^0.21.12",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/balanced-match": "^3.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.4",
    "@types/throttle-debounce": "^5.0.2",
    "balanced-match": "^3.0.1",
    "bumpp": "^9.2.1",
    "eslint": "npm:eslint-ts-patch@8.55.0-1",
    "eslint-ts-patch": "8.55.0-1",
    "esno": "^4.0.0",
    "lodash-es": "^4.17.21",
    "magic-regexp": "^0.7.0",
    "p-limit": "^5.0.0",
    "plv8ify": "^0.0.56",
    "rollup": "^4.7.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "throttle-debounce": "5.0.0",
    "type-level-regexp": "^0.1.17",
    "typescript": "^5.3.3",
    "ufo": "^1.4.0",
    "vite": "^5.0.7",
    "vitest": "^1.0.4",
    "xregexp": "^5.1.1"
  },
  "dependencies": {
    "@types/plv8-internal": "^3.1.2",
    "typescript-parsec": "^0.3.4"
  }
}
